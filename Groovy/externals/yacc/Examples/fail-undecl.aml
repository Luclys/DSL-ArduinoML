application foo {
  // Declaring bricks
  sensor button : 9;
  actuator led1 : 11;
  actuator led2 : 12;

  // Declaring states
  on {
    led <= HIGH;
    Led2 <= LOW;
    button2 is HIGH => off;
  }

  off {
    led3 <= LOW;
    led4 <= HIGH;
    button is HIGH => ON;
  }
}
